// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id String @id
  session Json
}

model BestMovies {
  id Int @id @default(autoincrement())
  movies Json
  updateDate DateTime
  type String
}